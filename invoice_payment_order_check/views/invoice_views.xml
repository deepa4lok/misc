<odoo>
  <data>

      <record id="payment_invoice_supplier_tree_inherit" model="ir.ui.view">
          <field name="name">account.invoice.supplier.tree.inherit</field>
          <field name="model">account.move</field>
          <field name="inherit_id" ref="account.view_in_invoice_bill_tree"/>
          <field name="arch" type="xml">
            <xpath expr="//field[@name='company_id']" position="before">
                    <field name="invoice_payment_order_id" groups="invoice_payment_order_check.group_payment_preview_manager"/>
            </xpath>
          </field>
      </record>
  </data>
</odoo>
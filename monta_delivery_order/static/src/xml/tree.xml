<?xml version="1.0" encoding="utf-8"?>

<templates id="template" xml:space="preserve">
<t t-name="MontaStockLot.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="primary" owl="1">
    <xpath expr="(//div/*)[last()]" position="after">
        <button t-if="props.resModel == 'monta.product.stock.lot'"
            type="button" class="btn btn-primary o_button_at_date me-2" t-on-click="onClickMontaStockLot">
            Get Monta Stock
        </button>
    </xpath>
</t>

</templates>

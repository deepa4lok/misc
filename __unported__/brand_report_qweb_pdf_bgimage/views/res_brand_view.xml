<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <!--
            Brand (form): Inherited
    -->
    <record id="res_brand_form_view" model="ir.ui.view">
        <field name="model">res.brand</field>
        <field name="inherit_id" ref="brand.res_brand_form_view" />
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="inside">
                <div colspan="2">
                    <label for="pdf_background_image" name="Background Image"/>
                    <field name="pdf_background_image" />
                </div>
            </xpath>
        </field>
    </record>
</odoo>
